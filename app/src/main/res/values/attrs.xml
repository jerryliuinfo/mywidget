<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!-- 圆形进度条 -->
    <declare-styleable name="circle_color_progress">
        <!-- 背景圆弧颜色 -->
        <attr name="bgArcColor" format="color|reference"/>
        <attr name="max_value" format="float"/>
        <attr name="start_angel" format="float"/>
        <attr name="sweep_angel" format="float"/>
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <!--背景颜色-->
        <attr name="background_color"  format="color|reference"/>
        <!--中间文字颜色-->
        <attr name="progress_text_color" format="color|reference"/>
        <!--中间文字大小-->
        <attr name="progress_text_size" format="dimension|reference"/>

        <!--进度条背景颜色-->
        <attr name="progress_background_color" format="color|reference"/>
        <!--进度条开始颜色-->
        <attr name="progress_start_color" format="color|reference"/>
        <!--进度条结束颜色-->
        <attr name="progress_end_color" format="color|reference"/>

        <attr name="progress_stroke_color" format="color|reference"/>
        <attr name="progress_stroke_width" format="dimension|reference"/>

        <!--针对线条的-->
        <attr name="progress_line_count" format="integer"/>
        <attr name="progress_line_width" format="dimension|reference"/>



        <attr name="style">
            <enum name="line" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="solid_line" value="2"/>
        </attr>

        <attr name="shader">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
            <enum name="sweep" value="2"/>
        </attr>

        <attr name="progress_stroke_cap">
            <enum name="butt" value="0"/>
            <enum name="round" value="1"/>
            <enum name="square" value="2"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="slideswitch">
        <attr name="slide_bg_open_color" format="color" />
        <attr name="slide_bg_close_color" format="color" />
        <attr name="slide_block_open_color" format="color" />
        <attr name="slide_block_close_color" format="color" />
        <attr name="themeColor" format="color" />

        <attr name="slide_circle_radis" format="dimension" />
        <attr name="padding_left" format="dimension" />
        <attr name="padding_top" format="dimension" />
        <attr name="corcle_padding_top" format="dimension" />

        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="MaterialEditText">
        <attr name="focusedLineColor" format="color"/>
        <attr name="labelText" format="string"/>
        <attr name="labelColor" format="color"/>
        <attr name="maxLength" format="integer"/>
        <attr name="overLengthColor" format="color"/>
    </declare-styleable>



    <declare-styleable name="HealthTable">
        <!--坐标轴线条出息-->
        <attr name="coordinatesLineWidth" format="dimension"/>
        <!--坐标轴字体大小-->
        <attr name="coordinatesTextSize" format="dimension" />
        <!--坐标轴轴字体颜色-->
        <attr name="coordinatesTextColor" format="color" />
        <!--折线颜色-->
        <attr name="lineColor" format="color" />
        <!--折线粗细-->
        <attr name="lineWidth" format="dimension" />
        <!--小圆点半径-->
        <attr name="averageCircleradius" format="dimension" />
        <!--表格的数据类型-->
        <attr name="tableType" format="string" />
        <!--大圆点的颜色-->
        <attr name="maxcircleColor" format="color" />
        <!--小圆点的颜色-->
        <attr name="mincircleColor" format="color" />
        <!--背景色-->
        <attr name="bgColor" format="color" />
    </declare-styleable>


    <declare-styleable name="my_slides_witch">
        <attr name="is_open" format="boolean" />
        <attr name="switch_shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="AvatorView">
        <attr name="format" format="enum">
            <enum name="circle" value="0"></enum>
            <enum name="rect" value="1"></enum>
        </attr>
    </declare-styleable>


    <declare-styleable name="SlantedTextView">
        <attr name="slantedMode" format="enum">
            <enum name="left" value="0"></enum>
            <enum name="right" value="1"></enum>
            <enum name="left_bottom" value="2"></enum>
            <enum name="right_bottom" value="3"></enum>
            <enum name="left_triangle" value="4"></enum>
            <enum name="right_triangle" value="5"></enum>
            <enum name="left_bottom_triangle" value="6"></enum>
            <enum name="right_bottom_triangle" value="7"></enum>
        </attr>
    </declare-styleable>

</resources>